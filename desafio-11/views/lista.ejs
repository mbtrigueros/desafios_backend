 <h2>Lista de productos</h2>

      <% if (!listaObject.state) { %>

        <h3> <%= listaObject.msg %> </h3>

      <% } else { %>

        <table class="table">
          <thead>
            <tr>
              <th scope="col">#id</th>
              <th scope="col">Nombre</th>
              <th scope="col">Precio</th>
              <th scope="col">Imagen</th>
            </tr>
          </thead>

          <tbody>

            <%  listaObject.list.forEach (function (producto) { %>
              <tr>
                <td><%=producto.id %></td>
                <td><%=producto.title %></td>
                <td><%=producto.price %></td>
                <td><img src="" alt="Imagen" srcset=<%=producto.thumbnail %>></td>
              </tr>
            <% }) %>

          </tbody>
        </table>
      <% } %>