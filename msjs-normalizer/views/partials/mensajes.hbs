    <h2> Mensajes </h2>
    <div id="mensajes"></div>
    <br>
    <form onsubmit="return envioMensaje(this)">
        <input id="mail" type="e-mail" placeholder="Ingrese su mail...">
        <input id="first-name" type="text" placeholder="Nombre" >
        <input id="last-name" type="text" placeholder="Apellido" >
        <input id="age" type="number" placeholder="Edad" >
        <input id="alias" type="text" placeholder="Alias" >
        <input id="avatar" type="url" placeholder="Avatar" >

        <input id="mensaje" type="text" placeholder="Su mensaje..." >
        <input type="submit">
    </form>